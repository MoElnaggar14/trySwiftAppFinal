  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Alvin Varghese": "alvinvarghese@live.com",
    "Natasha Murashev": "natasha@tryswift.co"
  },
  "source": {
    "git": "https://github.com/tryswift/trySwiftData.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "11.0",
    "watchos": "4.0",
    "tvos": "10.1"
  },
  "frameworks": [
    "UIKit",
    "Foundation"
  ],
  "default_subspecs": "TrySwiftAssets",
  "source_files": "TrySwiftData/**/*.{swift}",
  "subspecs": [
    {
      "name": "TrySwiftAssets",
      "resource_bundles": {
        "TrySwiftData": [
          "TrySwiftData/Assets/**/*.strings",
          "TrySwiftData/Assets/Conferences/**/*.{jpg,png,realm}"
        ]
      }
    },
    {
      "name": "Tokyo2017Lite",
      "resource_bundles": {
        "TrySwiftData": [
          "TrySwiftData/Assets/**/*.strings",
          "TrySwiftData/Assets/Conferences/*.{realm}"
        ]
      }
    }
  ]
}
